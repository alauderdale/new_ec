%section
  .container
    .row
      .col-lg-2
      .col-lg-8
        .text-center
          %h2
            This case study has come to an end!
          %h3
            I've prepared a few more for your viewing pleasure.
      .col-lg-2
#carousel-example-generic.carousel.slide{"data-ride" => "carousel"}
  / Indicators
  %ol.carousel-indicators
    %li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "3", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "4", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "5", "data-target" => "#carousel-example-generic"}
    %li{"data-slide-to" => "6", "data-target" => "#carousel-example-generic"}
  / Wrapper for slides
  .carousel-inner
    .item.active
      = partial "menu/logos"
    .item
      = partial "menu/outflow"
    .item
      = partial "menu/ucps"
    .item
      = partial "menu/mianzi"
    .item
      = partial "menu/chronotrack"
    .item
      = partial "menu/makeena"
    .item
      = partial "menu/wli"
      